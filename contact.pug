extends base.pug

block scripts
    script(src="../static/script.js")

block style
    style
      include ../static/style.css
      include ../static/stylecontact.css
     
block content
    
    div.container
        h1  Contact Us
        form(action="/contact", method="post" id="myform" onsubmit="geek()")
         input(type="text",class="myinput",name="name",placeholder="Enter your name")
         input(type="email",class="myinput",name="email",placeholder="Enter your email id")
         input(type="phone",class="myinput",name="phone",placeholder="Enter your phone no")
         input(type="text",class="myinput",name="address",placeholder="Enter your address")
         input(type="text",class="myinput",name="concern",placeholder="IF you have any concern")
         button(class="btn" )|Submit 
           
          